#The following example illustrates the PinFile
#
#SYNTAX:
#
#target:
#    topology: <string>
#    inventory: <string>

builder:
  topology: builder.yml
  layout: builder.yml

rhel7_nosec_nossl:
  topology: rhel7_nosec_nossl.yml
  layout: rhel7_nosec_nossl.yml

rhel7_nosec_ssl:
  topology: rhel7_nosec_ssl.yml
  layout: rhel7_nosec_ssl.yml

rhel7_sec_ssl:
  topology: rhel7_sec_ssl.yml
  layout: rhel7_sec_ssl.yml

rhel7_sec_nossl:
  topology: rhel7_sec_nossl.yml
  layout: rhel7_sec_nossl.yml

rhel7_sec_ssl_rhel6_slave:
  topology: rhel7_sec_ssl_rhel6_slave.yml
  layout: rhel7_sec_ssl_rhel6_slave.yml
